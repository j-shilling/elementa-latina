<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:my="urn:import:com.elementa.client"
	xmlns:res="urn:with:elementa.client.resources.Resources">
	
	<ui:with type="com.elementa.client.resources.AppResources" field="res">
	</ui:with>
	
	<g:FocusPanel ui:field="focusPanel">
		<g:HTMLPanel>
		
			<div class="{res.w3.Container}">
				<g:Label ui:field="searchLbl" addStyleNames="{res.theme.TextTheme}" />
				<g:TextBox ui:field="searchTxtBox"
					addStyleNames="{res.w3.Input} {res.w3.Border} {res.theme.Light5}"/>
			</div>
			
					
			<g:SimplePanel ui:field="tableContainer" addStyleNames="{res.w3.Container}" />
					
			<g:SimplePanel ui:field="addUserContainer" styleName="{res.w3.Hide}" />
					
			<div class="{res.w3.Container} {res.w3.Section}">
				<g:SimplePanel ui:field="saveBtnPanel" />
				<g:SimplePanel ui:field="addBtnPanel" />
			</div>
		
		</g:HTMLPanel>
	</g:FocusPanel>
</ui:UiBinder> 