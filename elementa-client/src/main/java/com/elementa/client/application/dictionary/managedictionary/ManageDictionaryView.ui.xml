<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui">
             
    <ui:with field="res" type="com.elementa.client.resources.AppResources"/>
    
	<g:FocusPanel ui:field="focus">
	    <g:HTMLPanel>
	    
	    	<g:SimplePanel ui:field="searchTxtBox" />
	    	<g:SimplePanel ui:field="table" />
	    	
	    	<div class="{res.w3.Container} {res.w3.CellRow}">
	    	
	    		<g:SimplePanel ui:field="listBox" addStyleNames="{res.w3.Padding} {res.w3.Cell} {res.w3.CellBottom} {res.w3.Mobile}" />
	    		<g:SimplePanel ui:field="newWordTxtBox" addStyleNames="{res.w3.Padding} {res.w3.Cell} {res.w3.CellBottom} {res.w3.Mobile}" />
	    		<g:SimplePanel ui:field="addBtn" addStyleNames="{res.w3.Padding} {res.w3.Cell} {res.w3.CellBottom} {res.w3.Mobile}" />
	    	
	    	</div>
	    
	    </g:HTMLPanel>
	</g:FocusPanel>
    
</ui:UiBinder>