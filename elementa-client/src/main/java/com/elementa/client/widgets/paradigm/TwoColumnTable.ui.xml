<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	
	<ui:with type="com.elementa.client.resources.AppResources" field="res">
	</ui:with>
	
	<g:HTMLPanel>
		
		<div class="{res.w3.Row}">
		
			<g:FlowPanel ui:field="left" addStyleNames="{res.w3.Half} {res.w3.Container} {res.w3.Left}" />
			<g:FlowPanel ui:field="right" addStyleNames="{res.w3.Half} {res.w3.Container} {res.w3.Right}" />
		
		</div>
		
	</g:HTMLPanel>
</ui:UiBinder> 